<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HasilActivity">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="180dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="73dp"
        android:layout_marginRight="10dp"
        android:id="@+id/upp"
        android:src="@drawable/upp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DATA MAHASISWA ILMU KOMPUTER"
        android:textSize="20dp"
        android:layout_marginTop="200dp"
        android:textColor="@color/black"
        android:layout_centerHorizontal="true"
        android:id="@+id/hasiljudul"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasiljudul"
        android:layout_marginTop="14dp"
        android:id="@+id/hasilnama"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasilnama"
        android:layout_marginTop="10dp"
        android:id="@+id/hasilnim"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasilnim"
        android:layout_marginTop="10dp"
        android:id="@+id/hasiltgl"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasiltgl"
        android:layout_marginTop="10dp"
        android:id="@+id/hasiljenis"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasiljenis"
        android:layout_marginTop="10dp"
        android:id="@+id/hasilprodi"/>




</RelativeLayout>